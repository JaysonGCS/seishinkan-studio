{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev-express": "tsx src/server.ts",
    "start-express": "cross-env NODE_ENV=production tsx src/server.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next_": "next build",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NEXT_BUILD=true node dist/server.js",
    "build:all": "npm run build:payload && npm run build:server && npm run build:next",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint ./src",
    "lint:debug": "eslint ./src --debug",
    "lint:fix": "eslint ./src --fix --ext .ts,.tsx",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier --write --ignore-unknown"
    ],
    "*.{json,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@payloadcms/bundler-webpack": "^1.0.6",
    "@payloadcms/db-postgres": "^0.8.2",
    "@payloadcms/eslint-config": "^1.1.1",
    "@payloadcms/plugin-seo": "^2.3.2",
    "@payloadcms/richtext-lexical": "^0.9.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@types/passport": "^1.0.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "deepmerge": "^4.3.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.368.0",
    "next": "14.2.3",
    "payload": "^2.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "react-loading-skeleton": "^3.4.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "winston": "^3.13.0",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.28",
    "@types/nodemailer": "^6.4.14",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "~14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-perfectionist": "^2.6.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tailwindcss": "^3.15.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.9.3",
    "typescript": "~5.4.5"
  }
}
